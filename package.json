{
  "name": "vibemaster",
  "version": "1.0.0",
  "description": "VibeMaster - Voice-driven game content creation and living world simulation",
  "main": "dist/index.js",
  "scripts": {
    "web": "npx live-server --port=5500",
    "dev": "npx tsx watch src/index.ts",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\" \"npm run tauri:dev\"",
    "dev:browser": "concurrently \"npm run dev:server\" \"npm run vite:dev\"",
    "check:tauri": "./check-tauri.sh",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "db:seed": "npx tsx src/seed.ts",
    "test": "npx tsx src/run-tests.ts",
    "test:db": "npx tsx src/run-tests.ts",
    "dev:server": "tsx watch src/dev-server.ts"
  },
  "keywords": [
    "gamedev",
    "voice",
    "ai",
    "decentraland",
    "simulation",
    "narrative"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "phaser": "^3.90.0",
    "vite": "^7.1.12"
  },
  "devDependencies": {
    "@prisma/client": "^6.18.0",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/cli": "^2.9.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^20.10.0",
    "concurrently": "^9.2.1",
    "live-server": "^1.2.2",
    "prisma": "^6.18.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
